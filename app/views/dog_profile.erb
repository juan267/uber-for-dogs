  <div id="content" class="clearfix">
    <section id="left">
      <div id="userStats" class="clearfix">
        <div class="pic">
          <a href="#"><img src="http://aaronwomack.com/wp-content/uploads/2011/01/SH-2-dog-ending-question-200x200.jpg" width="150" height="150" /></a>
        </div>

        <div class="data">
          <h1><%= @dog.name.capitalize %></h1>
          <div class="sep"></div>
          <ul class="numbers clearfix">
            <li>Distance Walk<strong><%= @dog.total_distance %></strong></li>
            <li>Calories Burn<strong>0</strong></li>
            <li class="nobrdr">Poop<strong>0</strong></li>
          </ul>
        </div>
      <a id="line" href="/coords">Draw line</a>
      </div>
      <% if @dog.walks.empty? %>
        <div id="no-walk-container">
          <h1>Schedule <%= @dog.name %> first walk!</h1>
          <form id ="new-walk"action="/owners/<%= current_owner.id %>/dogs/<%= @dog.id %>/walks" method="post">
            <label for="pick_up_time">Pick up time</label>
            <input type="datetime-local" name="pick_up_time"><br>
            <label for="time">Duration</label><br>
            <input type="radio" name="time" value="30" checked> 30 minutes<br>
            <input type="radio" name="time" value="60"> 60 minutes <br>
            <input type="submit" value="Schedule walk">
          </form>
        </div>
        <!--<a id="new-walk" href="/owners/<%= current_owner.id %>/dogs/<%= @dog.id %>/walks">New walk</a> -->
      <% elsif @walk.status = "active" %>
        <h1 id="dog-last-walk">Current walk:</h1>
        <a id ="dog-last-walk-path" href="/owners/<%= current_owner.id %>/dogs/<%= @dog.id %>/walks/active"></a>
        <div id="map-canvas">

        </div>
        <ul id="walks">

        </ul>

      <% else %>
        <h1 id="dog-last-walk">My last walk:</h1>
        <div id="map-canvas">

        </div>
        <ul id="walks">

        </ul>
      <% end %>


    </section>

    <section id="right">
      <div class="gcontent">
        <div class="head"><h1>Badges(3)</h1></div>
        <div class="boxy">
          <p>Keep walking to unlock badges!</p>

          <div class="badgeCount">
            <a href="#"><img src="http://fc06.deviantart.net/fs70/f/2013/225/1/2/pawicontealr_by_drache_lehre-d6i04io.png" /></a>
            <a href="#"><img src="http://fc06.deviantart.net/fs70/f/2013/225/1/2/pawicontealr_by_drache_lehre-d6i04io.png" /></a>
            <a href="#"><img src="http://fc06.deviantart.net/fs70/f/2013/225/1/2/pawicontealr_by_drache_lehre-d6i04io.png" /></a>
          </div>

          <span><a href="#">See allâ€¦</a></span>
        </div>
      </div>

      <div class="gcontent">
        <div class="head"><h1><%= @dog.name.capitalize %> Walkers</h1></div>
        <div class="boxy">
          <p>Total Walkers - <%= @walkers.count %></p>
          <div class="friendslist clearfix">
          <% @walkers.each do |walker| %>
             <div class="friend">
                <a href="#"><img src="http://fc05.deviantart.net/fs70/f/2010/206/3/9/link_icon_by_cezkid.gif" width="30" height="30" alt="Katie F." /></a><span class="friendly"><a href="/walkers/<%= walker.id %>"><%= walker.first_name %></a></span>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </section>
</div>