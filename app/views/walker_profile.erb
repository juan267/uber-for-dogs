  <div id="content" class="clearfix">
    <section id="left">
      <div id="userStats" class="clearfix">
        <div class="pic">
          <a href="#"><img src="http://www.gannett-cdn.com/-mm-/0132e47f007d456b307bc64195508eb524c0b070/c=0-331-4400-3639&r=x404&c=534x401/local/-/media/USATODAY/USATODAY/2014/08/19/1408462825000-NG-125-Cesar-Millan-3.jpg" width="150" height="150" /></a>
        </div>

        <div class="data">
          <h1><%= @walker.first_name.capitalize %></h1>
          <div class="sep"></div>
          <ul class="numbers clearfix">
            <li>Distance Walk<strong><%= @walker.total_distance %></strong></li>
            <li>Calories Burn<strong>344</strong></li>
          </ul>
        </div>
      <a id="line" href="/coords">Draw line</a>
      </div>

      <h1>Next walks</h1>
      <ul>
        <% @walker.walks.each do |walk| %>
          <ul>
            <li><%= walk.dog.name %></li>
            <li><%= walk.pick_up_time %></li>
            <% if current_walker %>
              <li><a href="/walkers/<%= current_walker.id %>/dogs/<%= walk.dog.id %>/walks/<%= walk.id %>/map">Start walk</a></li>
            <% end %>
          </ul>
        <% end %>
      </ul>

    </section>

    <section id="right">
      <div class="gcontent">
        <div class="head"><h1>Badges(3)</h1></div>
        <div class="boxy">
          <p>Keep walking to unlock badges!</p>

          <div class="badgeCount">
            <a href="#"><img src="http://icons.iconarchive.com/icons/seanau/fresh-web/512/Badge-icon.png" /></a>
            <a href="#"><img src="http://icons.iconarchive.com/icons/seanau/fresh-web/512/Badge-icon.png" /></a>
            <a href="#"><img src="http://icons.iconarchive.com/icons/seanau/fresh-web/512/Badge-icon.png" /></a>
          </div>
        </div>
      </div>

      <div class="gcontent">
        <div class="head"><h1><%= @walker.first_name.capitalize %> Dogs</h1></div>
        <div class="boxy">
          <p>Total Dogs - <%= @dogs.count %></p>
          <div class="friendslist clearfix">
          <% @dogs.each do |dog| %>
             <div class="friend">
                <a href="#"><img src="http://fc05.deviantart.net/fs70/f/2010/206/3/9/link_icon_by_cezkid.gif" width="30" height="30" alt="Katie F." /></a><span class="friendly"><a href="/owners/<%= dog.owner.id %>/dogs/<%= dog.id %>"><%= dog.name %></a></span>
              </div>
            <% end %>
          </div>
          <span><a href="#">See all...</a></span>
        </div>
      </div>
    </section>
  </div>